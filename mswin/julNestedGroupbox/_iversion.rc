            VALUE "ProductVersion",    julNestedGroupbox_VMAJORs "." julNestedGroupbox_VMINORs "." julNestedGroupbox_VPATCHs
            VALUE "LegalCopyright",    "Public domain"
            VALUE "OriginalFilename" , julNestedGroupbox_NAME ".exe"
            VALUE "InternalName", julNestedGroupbox_NAME ".exe"
            VALUE "Comments","Presented by public domain"
        }
    }

    BLOCK "VarFileInfo"
    {
        VALUE "Translation", 0x0409, 1252
    }
}
