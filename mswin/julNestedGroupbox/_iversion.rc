ProductVersion julNestedGroupbox_VMAJOR, julNestedGroupbox_VMINOR, julNestedGroupbox_VPATCH, julNestedGroupbox_VTAIL
{
    BLOCK "StringFileInfo"
    {
        BLOCK "040904E4"
        //language ID = U.S. English, char set = Windows, Multilingual
        {
            VALUE "CompanyName", "Public Domain"
            VALUE "ProductName",     julNestedGroupbox_NAME
            VALUE "FileDescription", julNestedGroupbox_NAME
